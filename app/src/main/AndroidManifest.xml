<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.freeler.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="demo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".rxjava.CreateActivity"
            android:label="Rxjava2创建操作符" />
        <activity
            android:name=".rxjava.MapActivity"
            android:label="Rxjava2变换操作符" />
        <activity
            android:name=".rxjava.FilterActivity"
            android:label="Rxjava2过滤操作符" />
        <activity
            android:name=".rxjava.BoolActivity"
            android:label="Rxjava2条件操作符" />
        <activity
            android:name=".annotation.MyFieldTest"
            android:label="Rxjava2条件操作符" />

    </application>

</manifest>